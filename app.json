{
  "expo": {
    "name": "capsulex-app-rn",
    "slug": "capsulex-app-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/capsulex-icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "capsulex",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capsulex.app",
      "associatedDomains": [
        "applinks:capsulex.app"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.capsulex.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "scheme": "capsulex"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "749cd921-deca-4e5e-8ef2-719846f4668f"
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "developmentClient": {
      "silentLaunch": true
    }
  }
}
